# Database
DATABASE_URL="file:./prisma/dev.db"

# JWT Secret (cambia questo valore in produzione!)
JWT_SECRET="your-super-secret-jwt-key-change-this-in-production"

# Server Configuration
PORT=4000
NODE_ENV=development

# CORS Origins (separati da virgola)
CORS_ORIGINS=http://localhost:5173,http://localhost:5174

# Google OAuth (opzionale - per integrazione Google Calendar/Gmail)
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
GOOGLE_REDIRECT_URI=http://localhost:4000/api/integrations/google/callback

# OpenAI API (opzionale - per Brain AI)
OPENAI_API_KEY=your-openai-api-key

# Anthropic API (opzionale - per Brain AI)
ANTHROPIC_API_KEY=your-anthropic-api-key

# Groq API (opzionale - per Brain AI)
GROQ_API_KEY=your-groq-api-key
